    "postversion": "git push origin master && git push --tags",
    "pretest": "npm run build",
    "test": "mocha ./test/index.js"
  },
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/justin-calleja/pkg-json-info-dict.git"
  },
  "keywords": [],
